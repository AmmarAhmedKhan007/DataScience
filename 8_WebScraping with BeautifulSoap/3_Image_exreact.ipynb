    "from PIL import Image\n",
    "import glob\n",
    "\n",
    "print(glob.glob('*.png'))\n",
    "for file in glob.glob(\"*.png\"):\n",
    "    im = Image.open(file)\n",
    "    rgb_im = im.convert('RGB')\n",
    "    rgb_im.save(file.replace(\"png\", \"jpg\"), quality=95)\n",
    "import os\n",
    "\n",
    "for file in glob.glob(\"*.png\"):\n",
    "    os.remove(file)"
   ]
  }
